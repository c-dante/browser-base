    "linter": "./node_modules/eslint/bin/eslint.js --fix src/ test/",
    "server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --progress",
    "test": "./node_modules/mocha/bin/mocha --inspect-brk -w --compilers js:babel-register src/**/*.spec.js",
    "web-test": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js \"mocha-loader!../test/allTests.js\" --target web --hot --inline --progress"
  },
  "dependencies": {},
