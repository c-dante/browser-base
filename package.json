    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.7.2",
    "chai": "^4.0.0",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "css-loader": "^0.28.1",
    "eslint": "^4.1.1",
    "eslint-import-resolver-webpack": "^0.8.3",
